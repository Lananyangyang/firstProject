*{margin:0 auto;padding:0}
body{font-size:14px;font-family: "arial, helvetica, sans-serif";line-height: 1.5em;text-align: center;}
img{display: block;}
a{color:inherit;text-decoration:none;cursor:pointer;}
input[type=text],input[type=password]{border-radius:3px;background: #fff;box-sizing:border-box;border: 1px #ddd solid;line-height: 2.5em;padding:0 10px;transition: all 0.3s ease-in;-moz-transition: all 0.3s ease-in;-webkit-transition: all 0.3s ease-in;-ms-transition: all 0.3s ease-in;}
input[type=button]{border-radius:3px;cursor:pointer;background: #54b5ff;color:#fff;line-height: 2.5em;outline: none;border: none;transition: all 0.3s ease-in;-moz-transition: all 0.3s ease-in;-webkit-transition: all 0.3s ease-in;-ms-transition: all 0.3s ease-in;}
input[type=button]:hover{opacity: 0.8;filter: alpha(opacity=80);}
input[type=text]:focus,input[type=password]:focus{border-color:#54b5ff;outline: none;}
h1,h2,h3,h4,h5,h6{font-weight:normal;line-height: 2em;}
ul,li,ol,dl,dt,dd{list-style: none;}
.clearfix:after{content:"";visibility: hidden;height:0;clear:both;display: block;}
.clearfix{display: block;zoom: 1;}

.loginBody{background: linear-gradient(#1888de,#54b5ff);background: -moz-linear-gradient(#1888de,#54b5ff);background: -webkit-linear-gradient(#1888de,#54b5ff);background: -ms-linear-gradient(#1888de,#54b5ff);}
.login{width:500px;position:fixed;top:50%;left:50%;margin:-140px 0 0 -250px;z-index: 10;}
.loginTitle{color:#fff;font-weight:bold;text-align: right;margin-bottom:5px}
.loginTitle h1{font-size:14px;border-bottom:1px #fff solid;}
.loginContent{background: #fff;border:1px #a7e2ff solid;padding:50px 30px 30px;box-sizing:border-box}
.loginItem{margin-bottom:20px;overflow: hidden;}
.loginItem p{float:left;width:120px;text-align:right;line-height: 2.5em;margin-right:20px}
.loginItem input[type=text],.loginItem input[type=password]{float:left;width: 240px;}
.loginItem #Login{width:200px;float:right;margin-right:58px;line-height:40px;border-radius:5px;color:#fff;background:#0e6ed1;cursor:pointer}
.loginItem #Login:hover{opacity:0.8;filter:alpha(opacity=80);transition:all 0.3s ease-out}
.loginFooter{position:fixed;bottom:0;left:0;line-height: 4em;width:100%;z-index:8}
.cloud{position:fixed;bottom:0;width:100%;}
.cloud .top{height:129px;background: url(../images/cloud1.png) repeat-x;}
.cloud .bottom{background: #fff;}
#cloud1{z-index: 6;}
#cloud1 .top{animation: cloud1 35s linear infinite;-moz-animation: cloud1 35s linear infinite;-webkit-animation: cloud1 35s linear infinite;-ms-animation: cloud1 35s linear infinite;}
#cloud2{z-index: 4;opacity: 0.6;}
#cloud2 .top{animation: cloud2 60s linear infinite;-moz-animation: cloud2 60s linear infinite;-webkit-animation: cloud2 60s linear infinite;-ms-animation: cloud2 60s linear infinite;}
#cloud2 .bottom{height:100px;}
#cloud3{z-index: 2;opacity: 0.3;}
#cloud3 .top{animation: cloud3 90s linear infinite;-moz-animation: cloud3 90s linear infinite;-webkit-animation: cloud3 90s linear infinite;-ms-animation: cloud3 90s linear infinite;}
#cloud3 .bottom{height:160px;}
.clouds{position:fixed;width:90px;height:24px;z-index: 1;background: url(../images/cloud2.png) no-repeat;}
#Clouds1{top:50px;animation: clouds1 55s linear infinite alternate;-moz-animation: clouds1 55s linear infinite alternate;-webkit-animation: clouds1 55s linear infinite alternate;-ms-animation: clouds1 55s linear infinite alternate;}
#Clouds2{top:150px;animation: clouds2 75s linear infinite alternate;-moz-animation: clouds2 75s linear infinite alternate;-webkit-animation: clouds2 75s linear infinite alternate;-ms-animation: clouds2 75s linear infinite alternate;}
#Clouds3{top:350px;animation: clouds3 125s linear infinite alternate;-moz-animation: clouds3 125s linear infinite alternate;-webkit-animation: clouds3 125s linear infinite alternate;-ms-animation: clouds3 125s linear infinite alternate;}

@keyframes cloud1{
	from{background-position: 0 0;}
	to{background-position: -1300px 0;}
}
